{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "skipLibCheck": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "noEmit": true,
  "jsx": "react-native",
  "lib": ["dom", "esnext"],
  "allowJs": true,
  "forceConsistentCasingInFileNames": true,
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
}
